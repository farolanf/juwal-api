{
  "connection": "default",
  "collectionName": "products",
  "info": {
    "name": "product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "title": {
      "required": true,
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "fields": {
      "collection": "fieldvalue"
    },
    "kabupaten": {
      "model": "kabupaten"
    },
    "category": {
      "model": "category"
    },
    "producttype": {
      "model": "producttype"
    },
    "owner": {
      "model": "user",
      "via": "products",
      "plugin": "users-permissions"
    },
    "images": {
      "type": "json"
    }
  }
}